@model SCE.Models.Part

@{
    ViewData["Title"] = "Create";
}

<h2>创建</h2>

<form asp-action="Create">
    <div class="form-horizontal">
        <h4>部门</h4>
        <hr />
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group">
            <label class="col-md-2 control-label">部门名称</label>
            <div class="col-md-10">
                <input asp-for="PartName" class="form-control" />
                <span asp-validation-for="PartName" class="text-danger" />
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="创建" class="btn btn-default" />
            </div>
        </div>
    </div>
</form>

<div>
    <a asp-action="Index">返回列表</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
